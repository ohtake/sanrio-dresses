  "private": true,
  "main": "src/index.jsx",
  "dependencies": {
    "css-element-queries": "^0.4.0",
    "justified-layout": "~2.0.0",
    "lodash": "^4.17.4",
    "material-ui": "^0.19.4",
    "object-fit-images": "^3.2.3",
    "prop-types": "^15.6.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-lazy-load": "^3.0.12",
    "react-router-dom": "^4.2.2",
    "react-swipeable": "^4.1.0",
    "verge": "^1.10.2"
  },
  "peerDependencies": {},
  "devDependencies": {
    "ava": "^0.22.0",
    "babel-cli": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-istanbul": "^4.1.5",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.10",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "codacy-coverage": "^2.0.3",
    "codeclimate-test-reporter": "^0.5.0",
    "coveralls": "^3.0.0",
    "cross-env": "^5.1.0",
    "enzyme": "^3.1.0",
    "enzyme-adapter-react-16": "^1.0.2",
    "esdoc": "^1.0.3",
    "esdoc-jsx-plugin": "^1.0.0",
    "esdoc-standard-plugin": "^1.0.0",
    "eslint": "^4.9.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-ava": "^4.2.2",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.4.0",
    "file-loader": "^1.1.5",
    "http-server": "^0.10.0",
    "jpeg-js": "^0.3.3",
    "js-yaml": "^3.10.0",
    "mkdirp": "^0.5.1",
    "npm-run-all": "^4.1.1",
    "nyc": "^11.2.1",
    "react-test-renderer": "^16.0.0",
    "request": "^2.83.0",
    "rimraf": "^2.6.2",
    "sinon": "^4.0.1",
    "source-map-explorer": "^1.5.0",
    "webpack": "^3.8.1",
    "webpack-dev-server": "^2.9.3",
    "yaml-loader": "^0.5.0"
  },
  "ava": {
    "require": [
      "babel-register",
      "./test/setup-enzyme.js"
    ],
    "babel": "inherit"
  },
  "scripts": {
    "clean": "rimraf assets/ coverage/ .nyc_output/",
    "lint": "eslint webpack.*.js src/* test/* scripts/*.js --color --report-unused-disable-directives",
    "test": "run-p -l test:*",
    "test:ava": "cross-env NODE_ENV=test ava test/**/*.ava.js",
    "cover": "cross-env NODE_ENV=test nyc ---extension .jsx --reporter lcov --reporter text yarn test",
    "watch:ava": "yarn run test:ava -- --watch",
    "build": "cross-env NODE_ENV=production npm-run-all -l -p build:app build:esdoc build:statistics -p build:licenses build:sme",
    "build:app": "webpack --progress --colors --config webpack.config.babel.js",
    "build:esdoc": "esdoc -c esdoc.json",
    "build:licenses": "yarn licenses generate-disclaimer --prod > assets/licenses.txt",
    "build:sme": "source-map-explorer assets/bundle.js assets/bundle.js.map --html > assets/sme.html",
    "build:statistics": "babel-node ./scripts/build-statistics.js",
    "start": "cross-env NODE_ENV=development webpack-dev-server --progress --colors --config webpack.config.babel.js",
    "server": "cross-env NODE_ENV=production http-server"
  }
}
