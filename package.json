  "dependencies": {
    "lodash": "4.17.4",
    "verge": "1.9.1",
    "css-element-queries": "0.4.0",
    "object-fit-images": "3.0.1",
    "react": "15.4.2",
    "react-dom": "15.4.2",
    "react-router": "3.0.2",
    "material-ui": "0.16.7",
    "react-tap-event-plugin": "2.0.1",
    "react-justified-layout": "0.0.1",
    "react-swipeable": "3.9.1",
    "react-lazy-load": "3.0.10"
  },
  "peerDependencies": {
  },
  "devDependencies": {
    "rimraf": "2.5.4",
    "babel-cli": "6.22.2",
    "babel-preset-es2015": "6.22.0",
    "babel-preset-react": "6.22.0",
    "babel-plugin-transform-react-remove-prop-types": "0.2.11",
    "cross-env": "3.1.4",
    "npm-run-all": "4.0.1",
    "eslint": "3.14.1",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-react": "6.9.0",
    "eslint-plugin-jsx-a11y": "3.0.2",
    "eslint-plugin-ava": "4.0.1",
    "eslint-config-airbnb": "14.0.0",
    "react-addons-test-utils": "15.4.2",
    "ava": "0.17.0",
    "sinon": "1.17.7",
    "enzyme": "2.7.1",
    "nyc": "10.1.2",
    "babel-plugin-istanbul": "3.1.2",
    "jsdoc": "3.4.3",
    "esdoc": "0.5.2",
    "source-map-explorer": "1.3.3",
    "request": "2.79.0",
    "js-yaml": "3.7.0",
    "codeclimate-test-reporter": "0.4.0",
    "coveralls": "2.11.15",
    "codacy-coverage": "2.0.1",
    "webpack": "2.2.1",
    "babel-loader": "6.2.10",
    "eslint-loader": "1.6.1",
    "yaml-loader": "0.4.0",
    "file-loader": "0.10.0",
    "webpack-dev-server": "1.16.3",
    "http-server": "0.9.0"
  },
  "ava": {
    "require": [
      "babel-register"
    ],
    "babel": "inherit"
  },
  "scripts": {
    "clean": "rimraf assets/ coverage/ .nyc_output/",
    "lint": "eslint webpack.*.js src/* test/* scripts/*.js --color",
    "test": "run-p -l test:*",
    "test:ava": "cross-env NODE_ENV=test ava test/**/*.ava.js",
    "cover": "cross-env NODE_ENV=test nyc ---extension .jsx --reporter lcov --reporter text npm test",
    "watch:ava": "npm run test:ava -- --watch",
    "build": "cross-env NODE_ENV=production npm-run-all -l -p build:app build:jsdoc build:esdoc -p build:sme",
    "build:app": "webpack --progress --colors --config webpack.config.babel.js",
    "build:jsdoc": "jsdoc --destination ./assets/jsdoc --readme README.md --verbose src",
    "build:esdoc": "esdoc -c esdoc.json",
    "build:sme": "source-map-explorer assets/bundle.js assets/bundle.js.map --html > assets/sme.html",
    "start": "cross-env NODE_ENV=development webpack-dev-server --progress --colors --config webpack.config.babel.js",
    "server": "cross-env NODE_ENV=production http-server"
  }
}
