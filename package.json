    "nyc": "^10.1.2",
    "react-addons-test-utils": "^15.4.2",
    "request": "^2.79.0",
    "rimraf": "^2.5.4",
    "sinon": "^1.17.7",
    "source-map-explorer": "^1.3.3",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.3.0",
    "yaml-loader": "^0.4.0"
  },
  "ava": {
    "require": [
      "babel-register"
    ],
    "babel": "inherit"
  },
  "scripts": {
    "clean": "rimraf assets/ coverage/ .nyc_output/",
    "lint": "eslint webpack.*.js src/* test/* scripts/*.js --color",
    "test": "run-p -l test:*",
    "test:ava": "cross-env NODE_ENV=test ava test/**/*.ava.js",
    "cover": "cross-env NODE_ENV=test nyc ---extension .jsx --reporter lcov --reporter text npm test",
    "watch:ava": "npm run test:ava -- --watch",
    "build": "cross-env NODE_ENV=production npm-run-all -l -p build:app build:jsdoc build:esdoc -p build:sme",
    "build:app": "webpack --progress --colors --config webpack.config.babel.js",
    "build:jsdoc": "jsdoc --destination ./assets/jsdoc --readme README.md --verbose src",
    "build:esdoc": "esdoc -c esdoc.json",
    "build:sme": "source-map-explorer assets/bundle.js assets/bundle.js.map --html > assets/sme.html",
    "start": "cross-env NODE_ENV=development webpack-dev-server --progress --colors --config webpack.config.babel.js",
    "server": "cross-env NODE_ENV=production http-server"
  }
}
