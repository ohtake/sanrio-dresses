  "dependencies": {
    "lodash": "4.17.2",
    "verge": "1.9.1",
    "css-element-queries": "0.3.2",
    "object-fit-images": "2.5.9",
    "react": "15.4.1",
    "react-dom": "15.4.1",
    "react-router": "3.0.0",
    "material-ui": "0.16.4",
    "react-tap-event-plugin": "2.0.1",
    "react-justified-layout": "0.0.1",
    "react-swipeable": "3.7.0",
    "react-lazy-load": "3.0.10"
  },
  "peerDependencies": {
  },
  "devDependencies": {
    "rimraf": "2.5.4",
    "babel-cli": "6.18.0",
    "babel-preset-es2015": "6.18.0",
    "babel-preset-react": "6.16.0",
    "babel-plugin-transform-react-remove-prop-types": "0.2.11",
    "cross-env": "3.1.3",
    "npm-run-all": "3.1.1",
    "eslint": "3.10.2",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-react": "6.7.1",
    "eslint-plugin-jsx-a11y": "2.2.3",
    "eslint-plugin-ava": "4.0.0",
    "eslint-config-airbnb": "13.0.0",
    "react-addons-test-utils": "15.4.1",
    "ava": "0.17.0",
    "sinon": "1.17.6",
    "enzyme": "2.6.0",
    "nyc": "10.0.0",
    "babel-plugin-istanbul": "3.0.0",
    "jsdoc": "3.4.3",
    "esdoc": "0.4.8",
    "source-map-explorer": "1.3.3",
    "request": "2.79.0",
    "js-yaml": "3.7.0",
    "codeclimate-test-reporter": "0.4.0",
    "coveralls": "2.11.15",
    "codacy-coverage": "2.0.0",
    "webpack": "1.13.3",
    "babel-loader": "6.2.8",
    "eslint-loader": "1.6.1",
    "yaml-loader": "0.4.0",
    "file-loader": "0.9.0",
    "webpack-dev-server": "1.16.2",
    "http-server": "0.9.0"
  },
  "ava": {
    "require": [
      "babel-register"
    ],
    "babel": "inherit"
  },
  "scripts": {
    "clean": "rimraf assets/ coverage/ .nyc_output/",
    "lint": "eslint webpack.*.js src/* test/* scripts/*.js --color",
    "test": "run-p -l test:*",
    "test:ava": "cross-env NODE_ENV=test ava test/**/*.ava.js",
    "cover": "cross-env NODE_ENV=test nyc ---extension .jsx --reporter lcov --reporter text npm test",
    "watch:ava": "npm run test:ava -- --watch",
    "build": "cross-env NODE_ENV=production npm-run-all -l -p build:app build:jsdoc build:esdoc -p build:sme",
    "build:app": "webpack --progress --colors",
    "build:jsdoc": "jsdoc --destination ./assets/jsdoc --readme README.md --verbose src",
    "build:esdoc": "esdoc -c esdoc.json",
    "build:sme": "source-map-explorer assets/bundle.js assets/bundle.js.map --html > assets/sme.html",
    "start": "cross-env NODE_ENV=development webpack-dev-server --progress --colors",
    "server": "cross-env NODE_ENV=production http-server"
  }
}
