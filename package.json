    "css-element-queries": "^1.0.2",
    "justified-layout": "^2.1.1",
    "lodash": "^4.17.10",
    "object-fit-images": "^3.2.4",
    "prop-types": "^15.6.2",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-lazy-load": "^3.0.13",
    "react-router-dom": "^4.3.1",
    "react-swipeable": "^4.3.0",
    "verge": "^1.10.2"
  },
  "peerDependencies": {},
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/node": "^7.0.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.0.0",
    "ava": "^1.0.0-beta.8",
    "babel-loader": "^8.0.0",
    "babel-plugin-istanbul": "^5.0.1",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.15",
    "codacy-coverage": "^3.0.0",
    "codeclimate-test-reporter": "^0.5.0",
    "codecov": "^3.0.4",
    "coveralls": "^3.0.2",
    "cross-env": "^5.2.0",
    "enzyme": "^3.5.0",
    "enzyme-adapter-react-16": "^1.3.0",
    "esdoc": "^1.1.0",
    "esdoc-jsx-plugin": "^1.0.0",
    "esdoc-standard-plugin": "^1.0.0",
    "eslint": "^5.4.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-loader": "^2.1.0",
    "eslint-plugin-ava": "^5.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.11.1",
    "file-loader": "^2.0.0",
    "http-server": "^0.11.1",
    "jpeg-js": "^0.3.4",
    "js-yaml": "^3.12.0",
    "mkdirp": "^0.5.1",
    "npm-run-all": "^4.1.3",
    "nyc": "^13.0.1",
    "react-test-renderer": "^16.4.2",
    "request": "^2.88.0",
    "request-promise-native": "^1.0.5",
    "rimraf": "^2.6.2",
    "sinon": "^6.1.5",
    "source-map-explorer": "^1.6.0",
    "webpack": "^4.17.1",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.6",
    "yaml-loader": "^0.5.0"
  },
  "ava": {
    "require": [
      "@babel/register",
      "@babel/polyfill",
      "./test/setup-enzyme.js"
    ]
  },
  "scripts": {
    "clean": "rimraf assets/ coverage/ .nyc_output/",
    "lint": "eslint webpack.*.js src/* test/* scripts/*.js --color --report-unused-disable-directives",
    "test": "run-p -l test:*",
    "test:ava": "cross-env NODE_ENV=test ava test/**/*.ava.js",
    "cover": "cross-env NODE_ENV=test nyc ---extension .jsx --reporter lcov --reporter text yarn test",
    "watch:ava": "yarn run test:ava -- --watch",
    "build": "cross-env NODE_ENV=production npm-run-all -l -p build:app build:esdoc build:statistics -p build:licenses build:sme",
    "build:app": "webpack --mode production --progress --colors --config webpack.config.babel.js",
    "build:esdoc": "esdoc -c esdoc.json",
    "build:licenses": "yarn licenses generate-disclaimer --prod > assets/licenses.txt",
    "build:sme": "source-map-explorer assets/bundle.js assets/bundle.js.map --html > assets/sme.html",
    "build:statistics": "babel-node ./scripts/build-statistics.js",
    "start": "run-p -l build:statistics start:devserver",
    "start:devserver": "cross-env NODE_ENV=development webpack-dev-server --mode development --progress --colors --config webpack.config.babel.js",
    "server": "cross-env NODE_ENV=production http-server"
  }
}
