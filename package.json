  "ava": {
    "require": [
      "@babel/register",
      "@babel/polyfill",
      "./test/setup-enzyme.js"
    ],
    "files": [
      "test/**/*.ava.js"
    ]
  },
  "scripts": {
    "clean": "rimraf assets/ coverage/ .nyc_output/",
    "lint": "eslint webpack.*.js src/* test/* scripts/*.js --color --report-unused-disable-directives",
    "test": "run-p -l test:*",
    "test:ava": "cross-env NODE_ENV=test ava",
    "cover": "cross-env NODE_ENV=test nyc ---extension .jsx --reporter lcov --reporter text npm run test",
    "watch:ava": "yarn run test:ava -- --watch",
    "build": "cross-env NODE_ENV=production npm-run-all -l -p build:app build:esdoc build:statistics -p build:licenses build:sme",
    "build:app": "webpack --mode production --progress --colors --config webpack.config.babel.js",
    "build:esdoc": "esdoc -c esdoc.json",
    "build:licenses": "yarn licenses generate-disclaimer --prod > assets/licenses.txt",
    "build:sme": "source-map-explorer assets/bundle.js assets/bundle.js.map --html > assets/sme.html",
    "build:statistics": "babel-node ./scripts/build-statistics.js",
    "start": "run-p -l build:statistics start:devserver",
    "start:devserver": "cross-env NODE_ENV=development webpack-dev-server --mode development --progress --colors --config webpack.config.babel.js",
    "server": "cross-env NODE_ENV=production babel-node ./scripts/server.js"
  }
}
