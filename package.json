    "file-loader": "0.9.0",
    "webpack-dev-server": "1.14.1",
    "http-server": "0.9.0"
  },
  "ava": {
    "require": [
      "babel-register"
    ],
    "babel": "inherit"
  },
  "nyc": {
    "sourceMap": false,
    "instrument": false
  },
  "scripts": {
    "clean": "rm -rf assets/ coverage/ .nyc_output/",
    "lint": "eslint webpack.*.js src/* test/* scripts/*.js --color",
    "test": "run-p -l test:*",
    "test:ava": "cross-env NODE_ENV=test ava test/**/*.ava.js",
    "cover": "cross-env NODE_ENV=test nyc ---extension .jsx --reporter lcov --reporter text npm test",
    "watch:ava": "npm run test:ava -- --watch",
    "build": "cross-env NODE_ENV=production run-p -l build:*",
    "build:app": "webpack --progress --colors",
    "build:jsdoc": "jsdoc --destination ./assets/jsdoc --readme README.md --verbose src",
    "start": "cross-env NODE_ENV=development webpack-dev-server --progress --colors",
    "server": "cross-env NODE_ENV=production http-server"
  }
}
